// https://codingbat.com/prob/p162010
// Return the number of even ints in a given array

#include <iostream>
#include <vector>

int main() {
    std::vector<int> vec = {1, 2, 3, 4, 5, 6};
    int count = 0;
    for (int n : vec)
    {
        if (n % 2 == 0)
        {
            count++;
        }
    }
    std::cout << count << std::endl;
    return 0;
}
