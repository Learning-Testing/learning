// codeacademy problem

bool sleep_in(bool weekday, bool vacation)
{
	if (weekday == false && vacation == false)
	{
		std::cout << "true";
		return true;
	}
	else if (weekday == true && vacation == false)
	{
		std::cout << "false";
		return false;
	}
	else
	{
		std::cout << "true";
		return true;
	}
}
